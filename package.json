{
  "description": "Lightweight, planet-scale, CDN-driven link shortener for teams ðŸŽ‰",
  "repository": "JaneJeon/blink",
  "author": "Jane Jeon <me@janejeon.dev>",
  "license": "AGPL-3.0",
  "proxy": "http://localhost:3000",
  "homepage": "/app",
  "scripts": {
    "user:promote": "node scripts/promote-user",
    "prestart": "NODE_ENV=development run-s build:yaml build:schema migrate seed --print-name",
    "start": "run-p start:* watch --race",
    "start:frontend": "BROWSER=none FORCE_COLOR=true PORT=4000 react-scripts start | cat",
    "start:backend": "nodemon node -r __utils__/dev-test-setup bin/www",
    "migrate": "knex migrate:latest",
    "migrate:one": "knex migrate:up",
    "migrate:make": "knex migrate:make",
    "migrate:list": "knex migrate:list",
    "rollback": "knex migrate:rollback",
    "rollback:one": "knex migrate:down",
    "seed": "knex seed:run",
    "seed:make": "knex seed:make --timestamp-filename-prefix",
    "build": "run-s build:*",
    "build:yaml": "yaml2json config/schema --save",
    "build:schema": "./scripts/compile-schema",
    "build:frontend": "INLINE_RUNTIME_CHUNK=false react-scripts build",
    "watch": "run-p watch:*",
    "watch:yaml": "onchange -k 'config/schema/*.yaml' -- npm run build:yaml",
    "watch:schema": "onchange -k 'config/schema/*.json' -- npm run build:schema",
    "watch:prettier": "onchange \"**/*\" -- prettier --write --ignore-unknown {{changed}}",
    "clean": "run-p clean:*",
    "clean:frontend": "rm -rf build",
    "clean:logs": "rm -rf logs/*.log",
    "clean:db": "knex seed:run --specific=20210125230719_clean.js",
    "lint": "run-s lint:* --print-name",
    "lint:prettier": "prettier --check . --ignore-path .gitignore",
    "lint:eslint": "eslint . --cache --ignore-pattern website/",
    "pretest": "run-s build:yaml build:schema --print-name",
    "test": "run-s test:nowatch:* --print-name",
    "test:watch": "run-s pretest test:watch:* --print-name",
    "test:nowatch:frontend": "CI=true npm run test:watch:frontend",
    "test:watch:frontend": "REACT_TEST=true react-scripts test",
    "test:nowatch:backend": "run-s migrate seed --print-name && jest --forceExit",
    "test:watch:backend": "npm run test:nowatch:backend --watch"
  },
  "dependencies": {
    "@casl/ability": "^5.1.1",
    "@passport-next/passport": "^3.1.0",
