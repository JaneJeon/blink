  "scripts": {
    "start": "NODE_ENV=production node bin/www",
    "dev": "NODE_ENV=development npm-run-all build-local -p dev:* watch --race",
    "dev:frontend": "react-scripts start",
    "dev:backend": "DEBUG=knex:query,ioredis:* nodemon bin/www",
    "migrate": "knex migrate:latest",
    "migrate:one": "knex migrate:up",
    "migrate:make": "knex migrate:make",
    "migrate:list": "knex migrate:list",
    "rollback": "knex migrate:rollback",
    "rollback:one": "knex migrate:down",
    "seed": "knex seed:run",
    "seed:make": "./scripts/make-seed.sh",
    "build": "run-p migrate build:*",
    "build-local": "npm-run-all -p migrate build:yaml -s seed",
    "build:yaml": "yaml2json config --save --recursive",
    "build:frontend": "react-scripts build",
    "watch": "run-p watch:*",
    "watch:yaml": "onchange -k 'config/**/*.yaml' -- npm run build:yaml",
    "lint": "run-s lint:*",
    "lint:prettier": "prettier --check .",
    "lint:eslint": "eslint .",
    "pretest": "NODE_ENV=test npm run build-local",
    "test": "run-s test:nowatch:*",
    "test:watch": "npm run pretest && run-s test:watch:*",
    "test:nowatch:frontend": "CI=true npm run test:watch:frontend",
    "test:watch:frontend": "react-scripts test",
    "test:nowatch:backend": "jest --forceExit",
    "test:watch:backend": "npm run test:nowatch:backend --watch"
  },
